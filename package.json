{
  "name": "@nateradebaugh/react-datetime",
  "version": "4.0.0-rc.29",
  "description": "A lightweight but complete datetime picker React.js component",
  "license": "MIT",
  "homepage": "https://github.com/NateRadebaugh/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/react-datetime"
  },
  "authors": [
    "Javier Marquez",
    "Nate Radebaugh"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/react-datetime.umd.production.min.js",
  "module": "dist/react-datetime.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "scss",
    "dist"
  ],
  "bundlesize": [
    {
      "path": "dist/react-datetime.cjs.production.min.js",
      "maxSize": "3.5 kB"
    },
    {
      "path": "dist/react-datetime.cjs.development.js",
      "maxSize": "10 kB"
    },
    {
      "path": "dist/react-datetime.esm.js",
      "maxSize": "5.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.production.min.js",
      "maxSize": "4.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.development.js",
      "maxSize": "10 kB"
    }
  ],
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "scripts": {
    "now-build": "yarn tsdx build",
    "build": "tsdx build --format=umd,esm,cjs",
    "dev": "tsdx watch --format=esm",
    "start": "yarn dev",
    "lint": "yarn eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "prettier": "prettier --write {src,scss}/*.{json,js,jsx,ts,tsx,scss} *.{json,js,md,mdx}",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn build"
  },
  "lint-staged": {
    "{src}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint src --ext .js,.ts,.jsx,.tsx --fix",
      "git add"
    ],
    "{src}/**/*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "{src}/**/*": [
      "bundlesize"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "bundlesize": "^0.18.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sass": "^1.23.0",
    "tsdx": "0.9.3",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@babel/core": "^7.6.3",
    "@reach/popover": "^0.3.0",
    "classcat": "^4.0.2",
    "date-fns": "^2.5.1",
    "use-onclickoutside": "^0.3.1"
  }
}
