{
  "name": "@nateradebaugh/react-datetime",
  "version": "4.0.0-rc.14",
  "description": "A lightweight but complete datetime picker React.js component",
  "license": "MIT",
  "homepage": "https://github.com/NateRadebaugh/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/react-datetime"
  },
  "authors": [
    "Javier Marquez",
    "Nate Radebaugh"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/react-datetime.umd.production.min.js",
  "module": "dist/react-datetime.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "scss",
    "dist"
  ],
  "bundlesize": [
    {
      "path": "dist/react-datetime.cjs.production.min.js",
      "maxSize": "3.2 kB"
    },
    {
      "path": "dist/react-datetime.cjs.development.js",
      "maxSize": "4.8 kB"
    },
    {
      "path": "dist/react-datetime.esm.js",
      "maxSize": "4.7 kB"
    },
    {
      "path": "dist/react-datetime.umd.production.min.js",
      "maxSize": "3.8 kB"
    },
    {
      "path": "dist/react-datetime.umd.development.js",
      "maxSize": "5.6 kB"
    }
  ],
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "scripts": {
    "now-build": "yarn tsdx build",
    "build": "tsdx build --format=umd,esm,cjs",
    "start": "tsdx watch --format=esm",
    "lint": "yarn eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "prettier": "prettier --write {src,scss}/*.{json,js,jsx,ts,tsx,scss}",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn build"
  },
  "lint-staged": {
    "{src}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint src --ext .js,.ts,.jsx,.tsx --fix",
      "git add"
    ],
    "{src}/**/*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "{src}/**/*": [
      "bundlesize"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "date-fns": ">=2.0.0",
    "react": ">=16.8",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "bundlesize": "^0.18.0",
    "cssnano": "^4.1.10",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.23.0-module.beta.1",
    "tsdx": "^0.9.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@reach/popover": "^0.1.0",
    "classcat": "^4.0.2",
    "date-fns": "^2.0.1",
    "use-onclickoutside": "^0.3.1"
  }
}
