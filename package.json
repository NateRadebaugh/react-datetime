{
  "name": "@nateradebaugh/react-datetime",
  "version": "3.0.1",
  "description": "A lightweight but complete datetime picker React.js component",
  "homepage": "https://github.com/NateRadebaugh/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/react-datetime"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "src",
    "css",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "rollup -c -w",
    "lint": "eslint **/*.{js,jsx}",
    "lint:fix": "eslint **/*.{js,jsx} --fix",
    "test": "cross-env CI=1 TZ=Europe/Stockholm react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=1 TZ=Europe/Stockholm react-scripts test --env=jsdom --coverage",
    "test:all": "echo 'Running tests...' && npm run test:coverage && echo 'All tests passed! ðŸ¤˜'",
    "test:watch": "cross-env TZ=Europe/Stockholm react-scripts test --env=jsdom",
    "prettier": "prettier --write **/*.{json,js,jsx,css}",
    "precommit": "lint-staged",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "cross-env CI=1 TZ=Europe/Stockholm react-scripts test --env=jsdom --bail --findRelatedTests"
    ],
    "*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "authors": [
    "Javier Marquez",
    "Nate Radebaugh"
  ],
  "license": "MIT",
  "peerDependencies": {
    "date-fns": "^1.29.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^16.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest-date-mock": "^1.0.3",
    "lint-staged": "^7.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.1",
    "react-dom": "^16.5.0",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.5.0",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0"
  },
  "dependencies": {
    "react-onclickoutside": "^6.7.1"
  }
}
