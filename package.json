{
  "name": "@nateradebaugh/react-datetime",
  "version": "4.3.0",
  "description": "A lightweight but complete datetime picker React.js component",
  "license": "MIT",
  "homepage": "https://github.com/NateRadebaugh/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/react-datetime"
  },
  "authors": [
    "Javier Marquez",
    "Nate Radebaugh"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/react-datetime.umd.production.min.js",
  "module": "dist/react-datetime.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "scss",
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "bundlesize": [
    {
      "path": "dist/react-datetime.cjs.production.min.js",
      "maxSize": "4.0 kB"
    },
    {
      "path": "dist/react-datetime.cjs.development.js",
      "maxSize": "10 kB"
    },
    {
      "path": "dist/react-datetime.esm.js",
      "maxSize": "5.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.production.min.js",
      "maxSize": "4.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.development.js",
      "maxSize": "10 kB"
    }
  ],
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "scripts": {
    "now-build": "build-storybook -c .storybook -o public",
    "build": "tsdx build --format=umd,esm,cjs",
    "dev": "tsdx watch --format=esm",
    "start": "yarn dev",
    "test": "yarn lint:fix&&cross-env CI=1 TZ=Europe/Stockholm tsdx test",
    "test:coverage": "yarn test --coverage",
    "test:all": "echo 'Running tests...' && yarn bundlesize && yarn test:coverage && echo 'All tests passed! ðŸ¤˜'",
    "test:watch": "cross-env TZ=Europe/Stockholm tsdx test --watch",
    "circleci-test": "cross-env CI=1 TZ=Europe/Stockholm JEST_JUNIT_OUTPUT_DIR=reports/junit tsdx test --ci --reporters=default --reporters=jest-junit --runInBand",
    "lint": "yarn eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write {src,scss}/*.{json,js,jsx,ts,tsx,scss} *.{json,js,md,mdx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "lint-staged": {
    "{src}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint src --ext .js,.ts,.jsx,.tsx --fix",
      "git add"
    ],
    "{src}/**/*.{json,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "{src}/**/*": [
      "bundlesize"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".stories.tsx"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.5",
    "@storybook/addon-actions": "5.3.0-rc.9",
    "@storybook/addon-docs": "5.3.0-rc.8",
    "@storybook/addon-info": "5.3.0-rc.9",
    "@storybook/addon-knobs": "5.3.0-rc.8",
    "@storybook/addon-links": "5.3.0-rc.9",
    "@storybook/addons": "5.3.0-rc.9",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "5.3.0-rc.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^8.0.0",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.4.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tsdx": "0.12.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@babel/core": "^7.6.3",
    "@reach/popover": "^0.7.0",
    "classcat": "^4.0.2",
    "date-fns": "^2.8.1",
    "tslib": "^1.10.0",
    "use-onclickoutside": "^0.3.1"
  }
}
