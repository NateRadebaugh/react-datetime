{
  "name": "@nateradebaugh/react-datetime",
  "version": "4.4.6",
  "description": "A lightweight but complete datetime picker React.js component",
  "license": "MIT",
  "homepage": "https://github.com/NateRadebaugh/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/react-datetime"
  },
  "authors": [
    "Javier Marquez",
    "Nate Radebaugh"
  ],
  "main": "dist/index.js",
  "umd:main": "dist/react-datetime.umd.production.min.js",
  "module": "dist/react-datetime.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "scss",
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "bundlesize": [
    {
      "path": "dist/react-datetime.cjs.production.min.js",
      "maxSize": "5.0 kB"
    },
    {
      "path": "dist/react-datetime.cjs.development.js",
      "maxSize": "10 kB"
    },
    {
      "path": "dist/react-datetime.esm.js",
      "maxSize": "6.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.production.min.js",
      "maxSize": "5.5 kB"
    },
    {
      "path": "dist/react-datetime.umd.development.js",
      "maxSize": "10 kB"
    }
  ],
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "scripts": {
    "now-build": "build-storybook -c .storybook -o public",
    "build": "tsdx build --format=umd,esm,cjs&&yarn sass scss/styles.scss dist/css/styles.css",
    "dev": "tsdx watch --format=esm",
    "start": "yarn dev",
    "test": "yarn lint:fix&&cross-env CI=1 TZ=Europe/Stockholm tsdx test",
    "test:ci": "yarn lint:fix&&cross-env CI=1 TZ=Europe/Stockholm tsdx test",
    "test:coverage": "yarn test --coverage",
    "test:all": "echo 'Running tests...' && yarn bundlesize && yarn test:coverage && echo 'All tests passed! ðŸ¤˜'",
    "test:watch": "cross-env TZ=Europe/Stockholm tsdx test --watch",
    "lint": "yarn eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "chromatic": "chromatic",
    "prepack": "yarn build"
  },
  "lint-staged": {
    "{src}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint src --ext .js,.ts,.jsx,.tsx --fix"
    ],
    "{src}/**/*.{json,css,scss}": [
      "prettier --write"
    ],
    "{src}/**/*": [
      "bundlesize"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".stories.tsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@storybook/addon-actions": "6.1.10",
    "@storybook/addon-docs": "6.1.12",
    "@storybook/addon-knobs": "6.1.10",
    "@storybook/addon-links": "6.1.11",
    "@storybook/addons": "6.1.10",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "6.1.10",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.0.0",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^26.0.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "chromatic": "^5.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-testing-library": "^3.8.0",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.0.0",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-dom": "^16.13.0",
    "react-is": "^17.0.1",
    "sass": "^1.26.3",
    "sass-loader": "^10.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^8.0.11",
    "tsdx": "0.14.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@reach/popover": "0.12.1",
    "classcat": "^4.0.2",
    "date-fns": "^2.16.1",
    "tslib": "^2.0.0",
    "use-onclickoutside": "^0.3.1"
  }
}
